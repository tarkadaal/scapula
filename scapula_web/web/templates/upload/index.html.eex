<div >
  <img class="welcome" src="images/welcome_title.png" />
  <%= form_for @conn, :upload, [multipart: true, as: :upload], fn f -> %>
    <div class="form-group">
    <%= file_input f, :data, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit "Upload!", class: "btn btn-primary" %>
  </div>
<% end %>
</div>